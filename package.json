{
  "name": "gulp-frontend-boilerplate",
  "title": "Gulp Frontend Boilerplate",
  "version": "1.6.2",
  "description": "An ES6 boilerplate with common frontend tasks using Gulp 4 as build system.",
  "author": "Damien Seguin <https://twitter.com/dmnsgn>",
  "homepage": "https://github.com/dmnsgn/gulp-frontend-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/dmnsgn/gulp-frontend-boilerplate"
  },
  "issues": {
    "url": "https://github.com/dmnsgn/gulp-frontend-boilerplate/issues"
  },
  "keywords": [
    "gulp",
    "gulp4",
    "es6",
    "es2015",
    "boilerplate",
    "frontend",
    "tasks",
    "workflow",
    "preprocessor",
    "framework"
  ],
  "license": "MIT",
  "vendors": [],
  "napa": {},
  "dependencies": {
    "normalize.css": "git://github.com/necolas/normalize.css.git#3.0.3"
  },
  "devDependencies": {
    "aliasify": "^2.1.0",
    "autoprefixer": "^9.4.x",
    "babel-core": "^6.26.x",
    "babel-preset-es2015": "^6.24.x",
    "brfs": "^2.0.1",
    "browser-sync": "^2.26.x",
    "browserify": "^16.2.x",
    "chalk": "^2.4.x",
    "css-mqpacker": "^7.0.x",
    "csswring": "^7.0.x",
    "del": "^3.0.x",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.x",
    "eslint-plugin-import": "^2.15.x",
    "eslint-plugin-jsx-a11y": "^6.1.x",
    "eslint-plugin-react": "^7.12.x",
    "favicons": "^5.3.x",
    "fontmin": "^0.9.x",
    "gulp": "^4.0.0",
    "gulp-cache": "^1.1.x",
    "gulp-changed": "^3.2.x",
    "gulp-filter": "^5.1.x",
    "gulp-header": "^2.0.x",
    "gulp-imagemin": "^5.0.x",
    "gulp-mocha": "^6.0.x",
    "gulp-newer": "^1.4.x",
    "gulp-notify": "^3.2.x",
    "gulp-postcss": "^8.0.x",
    "gulp-preprocess": "^3.0.x",
    "gulp-rename": "^1.4.x",
    "gulp-sitemap": "^6.1.x",
    "gulp-sourcemaps": "^2.6.x",
    "gulp-uglify": "^3.0.x",
    "gulp-util": "^3.0.x",
    "gulp-w3cjs": "^1.3.x",
    "gulp-watch": "^5.0.x",
    "gulp.spritesmith": "^6.9.x",
    "imagemin-pngquant": "^7.0.x",
    "minimist": "^1.2.x",
    "napa": "^3.0.x",
    "postcss": "^7.0.14",
    "pretty-hrtime": "^1.0.x",
    "psi": "^3.1.x",
    "stylelint": "^9.10.1",
    "stylelint-config-suitcss": "^14.0.0",
    "uglify-js": "^3.4.x",
    "vinyl-buffer": "^1.0.x",
    "vinyl-source-stream": "^2.0.x",
    "watchify": "^3.11.x"
  },
  "scripts": {
    "install": "napa",
    "init": "npm i inquirer@1.0.3 chalk cli-spinner del babel-cli babel-preset-es2015 && babel-node ./boilerplate/init/index.js",
    "dev": "NODE_ENV=development gulp --env dev",
    "prod": "NODE_ENV=production gulp --env prod"
  },
  "directories": {
    "src": "./src",
    "dist": "./dist",
    "test": "./test"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "plugins": [
      "react"
    ],
    "globals": {
      "ga": true
    },
    "rules": {
      "comma-dangle": 0
    }
  },
  "config": {
    "verbose": false,
    "port": 3000,
    "browsers": [
      "last 2 version",
      "safari 5",
      "ie 9",
      "opera 12.1",
      "ios 6",
      "android 4"
    ],
    "prodURL": "http://domain.tld",
    "shareImageURL": "http://domain.tld/static/share.png",
    "twitterImage": "https://domain.tld/static/twitter.png",
    "twitterHandle": "@domain",
    "themeColor": "#eeeeee",
    "analyticsUA": "UA-XXXXX-X",
    "developerURL": ""
  },
  "extensions": {
    "scripts": "js",
    "styles": "css"
  },
  "browserify": {
    "transform": [
      "aliasify",
      "brfs"
    ]
  },
  "aliasify": {
    "aliases": {
      "root": "./src/scripts",
      "views": "./src/scripts/views"
    }
  }
}
